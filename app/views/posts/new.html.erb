<h1>New Post</h1>
<div class="row">
	<div class = "span6 offset3">
		<%= form_for(@post) do |f| %>
			<%= f.label :title %>
			<%= f.text_field :title %>
			<%= f.label :body %>
			<%= f.text_area :body %>
	      	
	      	<%= f.submit "Submit", class: "btn btn-large btn-primary" %>
    	<% end %>
	</div>
</div>
